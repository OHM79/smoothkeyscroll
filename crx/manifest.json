{
  "name": "Smooth Key Scroll",
  "version": "1.0.3.7",
  "description": "Smooth scroll with the keyboard",
  "offline_enabled": true,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches":["*://aristidesfl.github.com/smoothkeyscroll*"],
      "js": ["inject.js"]
    }
  ],
  "permissions": [
     "storage",
     "alarms",
     "*://smoothkeyscroll.herokuapp.com/*"

   ],
   "background": {
     "scripts": ["eventPage.js"],
     "persistent": false
   },
  "options_page": "options/options.html",
  "manifest_version": 2,
  "homepage_url": "http://aristidesfl.github.com/smoothkeyscroll/",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://wallet.google.com https://ajax.googleapis.com https://www.google.com/uds/; object-src 'self'"
}
